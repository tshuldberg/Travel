<%- include('../partials/head') %>
<nav class="header">
    <div class="nav-wrapper">
      <a href="/users" class="my-page"><i class="fas fa-globe-americas fa-3x"></i></a>

      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><a href="/destinations">Where Ya Goin</a></li>
      </ul>
    </div>
  </nav>


<% posts.forEach(function(p) { %>

    <div class="posts">
        <div class="row">
            <div class="col s12 m7">
                <div class="card blue-grey darken-1">
                    <div class="card-image">
                        <img src=https://images.unsplash.com/photo-1516738901171-8eb4fc13bd20?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1950&q=80>
                        <span class="card-title"> <%= p.title %></span>
                    </div>
                    <div class="card-content">
                        <div><%= p.textContent %></div>
                        <div><%= p.datePosted %></div>
                        <div>Where Ya Goin!    <%= p.destination %></div>
                    </div>
                    <div class="card-action">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <%  }) %>